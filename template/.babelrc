{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "useBuiltIns": false,
        "targets": {
          "browsers": [
            "defaults",
            "ios >= 7",
            "not ie <= 100",
            "not ExplorerMobile <= 100",
            "not Opera <=100",
            "not OperaMini <= 100",
            "not OperaMini all",
            "not OperaMobile <= 100",
            "not BlackBerry <= 100"
          ]
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime"
  ],
  "comments": false
}
